HeaderName HEADER.html
IndexOptions FancyIndexing VersionSort

Options ExecCGI Indexes FollowSymLinks
#AllowOverride All

AddHandler cgi-script .sh
#AddHandler cgi-script .pl

<IfModule mod_auth_kerb.c>
	Require valid-user
</IfModule>


<Files nph-pdb-fetch.sh>
    Order allow,deny
    Allow from all
    Satisfy any
</Files>

